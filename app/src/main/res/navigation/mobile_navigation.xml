<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation.xml"
    app:startDestination="@id/profileFragment">

        <fragment
            android:id="@+id/profileFragment"
            android:name="com.castprogramms.balamutbatut.ui.profile.ProfileStudentFragment"
            tools:layout="@layout/fragment_profile_student" >
                <action
                    android:id="@+id/action_profileFragment_to_settingsFragment"
                    app:destination="@id/settingsFragment"
                    app:enterAnim="@anim/left_out"
                    app:exitAnim="@anim/right_out"
                    app:popEnterAnim="@anim/left_in"
                    app:popExitAnim="@anim/right_in"/>
                <action
                    android:id="@+id/action_profileFragment_to_editProfileFragment"
                    app:destination="@id/editProfileFragment"
                    app:enterAnim="@anim/left_out"
                    app:exitAnim="@anim/right_out"
                    app:popEnterAnim="@anim/left_in"
                    app:popExitAnim="@anim/right_in"/>
                <action
                    android:id="@+id/action_profileFragment_to_qrCodeFragment"
                    app:destination="@id/qrCodeFragment"
                    app:enterAnim="@anim/left_out"
                    app:exitAnim="@anim/right_out"
                    app:popEnterAnim="@anim/left_in"
                    app:popExitAnim="@anim/right_in"/>
        </fragment>
        <fragment
            android:id="@+id/settingsFragment"
            android:name="com.castprogramms.balamutbatut.ui.settings.SettingsFragment"
            tools:layout="@layout/settings_fragment" >
                <action
                    android:id="@+id/action_settingsFragment_to_qrCodeFragment"
                    app:destination="@id/qrCodeFragment"
                    app:enterAnim="@anim/left_out"
                    app:exitAnim="@anim/right_out"
                    app:popEnterAnim="@anim/left_in"
                    app:popExitAnim="@anim/right_in"/>
        </fragment>
        <fragment
            android:id="@+id/editProfileFragment"
            android:name="com.castprogramms.balamutbatut.ui.editprofile.EditProfileFragment"
            tools:layout="@layout/fragment_edit_profile" />
        <fragment
            android:id="@+id/ratingFragment"
            android:name="com.castprogramms.balamutbatut.ui.rating.rating_student.RatingStudentFragment"
            tools:layout="@layout/fragment_rating_student">
                <action
                    app:enterAnim="@anim/left_out"
                    app:exitAnim="@anim/right_out"
                    app:popEnterAnim="@anim/left_in"
                    app:popExitAnim="@anim/right_in"
                    android:id="@+id/action_ratingFragment_to_showGroupElementFragment"
                    app:destination="@id/showGroupElementFragment" />
        </fragment>
        <fragment
            android:id="@+id/qrCodeFragment"
            android:name="com.castprogramms.balamutbatut.ui.qrcode.QrCodeFragment"
            tools:layout="@layout/fragment_qr_code"
            />
        <fragment
            android:id="@+id/allElementListFragment"
            android:name="com.castprogramms.balamutbatut.ui.allElements.AllElementListFragment"
            tools:layout="@layout/all_element_list_fragment" >
                <action
                    android:id="@+id/action_allElementListFragment_to_allGroupElementFragment2"
                    app:destination="@id/allGroupElementFragment2"
                    app:enterAnim="@anim/left_out"
                    app:exitAnim="@anim/right_out"
                    app:popEnterAnim="@anim/left_in"
                    app:popExitAnim="@anim/right_in"/>
        </fragment>
        <fragment
            android:id="@+id/allGroupElementFragment2"
            android:name="com.castprogramms.balamutbatut.ui.allgroupelement.AllGroupElementFragment"
            tools:layout="@layout/fragment_group_elements">
                <action
                    android:id="@+id/action_allGroupElementFragment2_to_infoElementFragment2"
                    app:destination="@id/infoElementFragment2"
                    app:enterAnim="@anim/left_out"
                    app:exitAnim="@anim/right_out"
                    app:popEnterAnim="@anim/left_in"
                    app:popExitAnim="@anim/right_in"/>
        </fragment>
        <fragment
            android:id="@+id/infoElementFragment2"
            android:name="com.castprogramms.balamutbatut.ui.infoelementfragment.InfoElementFragment"
            tools:layout="@layout/fragment_info_element"/>
        <fragment
            android:id="@+id/showGroupElementFragment"
            android:name="com.castprogramms.balamutbatut.ui.showelements.ShowGroupElementsFragment"
            tools:layout="@layout/fragment_group_elements">
                <action
                    android:id="@+id/action_showGroupElementFragment_to_showElementsFragment3"
                    app:destination="@id/showElementsFragment3"
                    app:enterAnim="@anim/left_out"
                    app:exitAnim="@anim/right_out"
                    app:popEnterAnim="@anim/left_in"
                    app:popExitAnim="@anim/right_in"/>
        </fragment>
        <fragment
            android:id="@+id/showElementsFragment3"
            android:name="com.castprogramms.balamutbatut.ui.showelements.ShowElementsFragment"
            tools:layout="@layout/fragment_add_elements">
                <action
                    android:id="@+id/action_showElementsFragment3_to_infoElementFragment2"
                    app:destination="@id/infoElementFragment2"
                    app:enterAnim="@anim/left_out"
                    app:exitAnim="@anim/right_out"
                    app:popEnterAnim="@anim/left_in"
                    app:popExitAnim="@anim/right_in"/>
        </fragment>

</navigation>